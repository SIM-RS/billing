            $sqlmsn = "SELECT asunit.namaunit ,asunit.kodeunit, asbrg.idbarang, asbrg.kodebarang, 
                        seri.noseri, asbrg.namabarang, seri.jenis_kib, kib.merk, asbrg.tipebarang, 
                        seri.kondisi, kib.no_pabrik, kib.no_rangka, kib.no_mesin, kib.no_polisi, 
                        kib.no_bpkb,kib.ket, seri.asalusul, seri.harga_perolehan, lks.namalokasi,
                         lks.idlokasi,lks.kodelokasi 
                        FROM as_seri2 seri
                        LEFT JOIN as_ms_unit asunit ON seri.ms_idunit = asunit.idunit
                        LEFT JOIN as_ms_barang asbrg ON seri.idbarang = asbrg.idbarang
                        LEFT JOIN kib02 kib ON seri.idseri = kib.idseri
                        LEFT JOIN as_lokasi lks ON seri.ms_idlokasi = lks.idlokasi
                        WHERE seri.idseri =$idkib";
            $dtmsn = mysql_query($sqlmsn);
            $rwmsn = mysql_fetch_array($dtmsn);
